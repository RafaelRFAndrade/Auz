{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "#{CONNECTION_STRING}#"
  },
  "Jwt": {
    "Key": "#{JWT_KEY}#",
    "Issuer": "Auz",
    "Audience": "Auz/Usuario",
    "ExpireMinutes": 60
  },
  "AWS": {
    "AccessKey": "#{AWS_ACCESS_KEY}#",
    "SecretKey": "#{AWS_SECRET_KEY}#",
    "Region": "sa-east-1",
    "Bucket": "auzys-documentos"
  }
}